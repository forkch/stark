function set_color_custom
    set -U fish_color_normal                normal
    set -U fish_color_command               fc3
    set -U fish_color_param                 fff
    set -U fish_color_redirection           99f
    set -U fish_color_comment               30f
    set -U fish_color_error                 f00
    set -U fish_color_escape                fc3
    set -U fish_color_operator              fc3
    set -U fish_color_end                   fff
    set -U fish_color_quote                 99f
    set -U fish_color_autosuggestion        555
    set -U fish_color_valid_path            --underline
    set -U fish_color_cwd                   fc3
    set -U fish_color_cwd_root              fc3
    set -U fish_color_match                 fc3
    set -U fish_color_search_match          --background=30f
    set -U fish_color_selection             --background=30f
    set -U fish_pager_color_prefix          fc3
    set -U fish_pager_color_completion      fff
    set -U fish_pager_color_description     555 fc3
    set -U fish_pager_color_progress        fc3
    set -U fish_color_history_current       fc3
end
